type Query {
    students: [Student]
    student(id: Int!): Student
    findStudentsByFirstName(firstName: String!): [Student]
}

type Mutation {
    createStudent(student: StudentInput): Student
}

input StudentInput {
    id: Int!
    firstName: String!
    lastName: String!
    registration: String!
}

type Student {
    id: Int!
    firstName: String!
    lastName: String!
    registration: String!
    papers: [Paper]
}

type Paper {
    id: Int!
    title: String!
}